	id		= "com.phonegap.getting.started"
	version 	= "1.0.0">

	<name>Slatwall</name>

	<description>
		This is a mobile toolset for Slatwall
	</description>

	<author href="http://www.gregmoser.com"
		email="greg@gregmoser.com">
		Greg Moser
	</author>

	<icon src="resources/icons/icon.png" gap:role="default" />

	<feature name="http://api.phonegap.com/1.0/geolocation"/>
